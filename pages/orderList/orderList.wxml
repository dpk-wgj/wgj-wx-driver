<view class="main">
    <view class="bd" wx:for="{{10}}" wx:key="i" wx:for-index="index">
      <view class="total">
          <view class="total-time">
              <view class="img">
                  <image src="../../assets/images/clock.png"></image>
              </view>
              <text class="time">{{orderList[0].startTime}}</text>
          </view>
          <view class="total-start">
              <view class="start-pic"></view>
              <text class="desc">{{orderList[0].startLoc}}</text>
          </view>
          <view class="total-end">
              <view class="end-pic"></view>
              <text class="desc">{{orderList[0].endLoc}}</text>
          </view>
          
          <view class="total-state" bindtap="toRules">
              已完成 >
          </view>
      </view>
